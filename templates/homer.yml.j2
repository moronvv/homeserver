# Services
# First level array represent a group.
# Leave only a "items" key if not using group (group name, icon & tagstyle are optional, section separation will not be displayed).
services:
  - name: "Network"
    icon: "fas fa-network-wired"
    items:
      - name: "Pi-hole"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/pihole.png"
        url: "https://pihole.{{ domain_name }}/admin/"
        target: "_blank"
        type: "PiHole"
      - name: "SWAG Dashboard"
        logo: "https://raw.githubusercontent.com/linuxserver/docker-templates/master/linuxserver.io/img/linuxserver-ls-logo.png"
        url: "https://dashboard.{{ domain_name }}/"
        target: "_blank"
  - name: "Applications"
    icon: "fas fa-cloud"
    items:
      - name: "Vaultwarden"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/bitwarden.png"
        url: "https://vaultwarden.{{ domain_name }}/"
        target: "_blank"
        type: "Ping"
      - name: "Nextcloud"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/nextcloud.png"
        url: "https://nextcloud.{{ domain_name }}/"
        target: "_blank"
        type: "Ping"
      - name: "Photoprism"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/photoprism.png"
        url: "https://photoprism.{{ domain_name }}/"
        target: "_blank"
        type: "Ping"
      - name: "qBittorrent"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/qbittorrent.png"
        url: "https://qbittorrent.{{ domain_name }}/"
        target: "_blank"
      - name: "Calibre-Web"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/calibreweb.png"
        url: "https://calibre-web.{{ domain_name }}/"
        target: "_blank"
  - name: "Media"
    icon: "fas fa-photo-video"
    items:
      - name: "Jellyfin"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/jellyfin.png"
        url: "https://jellyfin.{{ domain_name }}/"
        target: "_blank"
      - name: "Navidrome"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/navidrome.png"
        url: "https://navidrome.{{ domain_name }}/"
        target: "_blank"
      - name: "Mopidy"
        logo: "https://raw.githubusercontent.com/jaedb/Iris/master/src/assets/app-icon_256.png"
        url: "https://mopidy.{{ domain_name }}/"
        target: "_blank"
      - name: "Radarr"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/radarr.png"
        url: "https://radarr.{{ domain_name }}/"
        target: "_blank"
        apikey: "{{ radarr_api_key }}"
        type: "Radarr"
      - name: "Sonarr"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/sonarr.png"
        url: "https://sonarr.{{ domain_name }}/"
        target: "_blank"
        apikey: "{{ sonarr_api_key }}"
        type: "Sonarr"
      - name: "Lidarr"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/lidarr.png"
        url: "https://lidarr.{{ domain_name }}/"
        target: "_blank"
        apikey: "{{ lidarr_api_key }}"
        type: "Lidarr"
      - name: "Readarr"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/readarr.png"
        url: "https://readarr.{{ domain_name }}/"
        target: "_blank"
      - name: "Jackett"
        logo: "https://raw.githubusercontent.com/NX211/homer-icons/master/png/jackett.png"
        url: "https://jackett.{{ domain_name }}/"
        target: "_blank"
